<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>NUnitTDNet</id>
    <title>NUnit $version$ framework for use with TestDriven.Net</title>
    <version>$version$.$BuildNumber$$PreRelease$</version>
    <authors>Jamie Cansdale, Charlie Poole</authors>
    <owners>Jamie Cansdale</owners>
    <licenseUrl>https://nunit.org/nuget/nunit3-license.txt</licenseUrl>
    <projectUrl>https://testdriven.net</projectUrl>
    <repository type="git" url="https://github.com/nunit/nunit3-tdnet-adapter"/>
    <iconUrl>https://nunit.org/nuget/nunitv3_32x32.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <summary>NUnit $version$ framework and test runner adapter for TestDriven.Net</summary>
    <description>This package includes the NUnit $version$ framework assembly, which is referenced by your tests. It also includes a test runner adapter that is used by TestDriven.Net when executing tests. The adapter is named 'nunit.framework.dll.tdnet' and must live in the same directory as the referenced 'nunit.framework.dll'.</description>
    <releaseNotes>Update for NUnit $version$</releaseNotes>
    <language>en-US</language>
    <tags>nunit testdriven tdnet test testing tdd framework addin</tags>
    <copyright>Copyright (c) 2016 Jamie Cansdale, Charlie Poole</copyright>
    <references>
      <reference file="nunit.framework.dll" />
    </references>
  </metadata>
  <files>
    <file src="$OutputPath$\TestDriven.Framework.dll" target="tools" />
    <file src="$OutputPath$\NUnitTDNet.Adapter.dll" target="tools" />
    <file src="$PackagesPath$\NUnit.Engine.$version$\lib\*" target="tools" />
    <file src="$OutputPath$\nunit.framework.dll.tdnet" target="lib\net20" />
    <file src="$PackagesPath$\NUnit.$version$\lib\net20\*" target="lib\net20" />
    <file src="$OutputPath$\nunit.framework.dll.tdnet" target="lib\net35" />
    <file src="$PackagesPath$\NUnit.$version$\lib\net35\*" target="lib\net35" />
    <file src="$OutputPath$\nunit.framework.dll.tdnet" target="lib\net40" />
    <file src="$PackagesPath$\NUnit.$version$\lib\net40\*" target="lib\net40" />
    <file src="$OutputPath$\nunit.framework.dll.tdnet" target="lib\net45" />
    <file src="$PackagesPath$\NUnit.$version$\lib\net45\*" target="lib\net45" />
    <file src="$PackagesPath$\NUnit.$version$\*.txt" />
  </files>
</package>
